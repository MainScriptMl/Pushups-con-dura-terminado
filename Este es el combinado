-- Lista de URLs RAW (reemplaza con las verdaderas)
local scripts = {
  
    "https://raw.githubusercontent.com/MainScriptMl/Flexiones-con-dura/refs/heads/main/Pushups%20con%20dura",

"https://raw.githubusercontent.com/CTXxALEJANDROxX/Roca-10m/refs/heads/main/Roca%2010m",
 
"https://raw.githubusercontent.com/CTXxALEJANDROxX/Auto-fuerza-OP-2.0/refs/heads/main/Auto%20fuerza",
    }

for _, url in ipairs(scripts) do
    local success, result = pcall(function()
        local code = game:HttpGet(url)
        return loadstring(code)()
    end)
    
    if success then
        print("✅ Script ejecutado:", url)
    else
        warn("❌ Error al ejecutar script:", url, "\n", result)
    end
end
